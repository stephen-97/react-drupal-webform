  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": [
    "**/*.scss"
  ],
  "peerDependencies": {
    "@tippyjs/react": "^4.2.6",
    "react": ">=18",
    "react-dom": ">=18",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sass": "^1.94.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.2"
  }
