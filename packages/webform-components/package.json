{
  "name": "webform-components",
  "version": "1.0.1-alpha",
  "description": "React components for webform Drupal",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.build.json && npm run copy:scss",
    "copy:scss": "node copy-scss.js"
  },
  "files": [
    "dist",
    "src/**/*.scss"
  ],
